{
  "name": "spoke",
  "private": true,
  "productName": "Spoke",
  "license": "MPL-2.0",
  "version": "0.8.6",
  "engines": {
    "node": ">=10",
    "npm": ">=6.4"
  },
  "main": "lib/server/index.js",
  "files": [
    "lib/",
    "dist/",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/Spoke.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "bundle-analyzer": "cross-env NODE_ENV=production webpack-dev-server --mode=production --env.BUNDLE_ANALYZER",
    "precommit": "lint-staged",
    "lint": "eslint ./src",
    "test": "concurrently --success \"first\" --kill-others \"npm:test-all\" \"npm:test-server\"",
    "test-server": "cross-env NODE_ENV=test webpack-dev-server --mode development --port 9091 --content-base ./test/fixtures",
    "test-all": "ava",
    "unit-tests": "ava ./test/unit",
    "integration-tests": "concurrently --success \"first\" --kill-others \"ava ./test/integration\" \"npm:test-server\"",
    "update-test-snapshots": "concurrently --success \"first\" --kill-others \"ava -u\" \"npm:test-serve\""
  },
  "ava": {
    "files": [
      "./test/unit",
      "./test/integration"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register",
      "esm",
      "./test/entry.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@mozillareality/easing-functions": "^0.1.0",
    "@mozillareality/three-particle-emitter": "^0.1.3",
    "@robertlong/react-ui-tree": "github:robertlong/react-ui-tree#dev",
    "@sentry/browser": "^5.1.1",
    "abortcontroller-polyfill": "^1.3.0",
    "classnames": "^2.2.5",
    "eventemitter3": "^3.1.0",
    "evergreen-ui": "^4.15.0",
    "form-data": "^2.3.3",
    "fuse.js": "^3.4.4",
    "hls.js": "^0.12.3",
    "jszip": "^3.2.0",
    "jwt-decode": "^2.2.0",
    "mousetrap": "^1.6.3",
    "phoenix": "^1.4.1",
    "prop-types": "^15.7.2",
    "rc-slider": "^8.6.6",
    "react": "^16.8.3",
    "react-color": "^2.17.0",
    "react-contextmenu": "^2.10.0",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-modal": "^3.8.1",
    "react-mosaic-component": "^2.1.0",
    "react-router-dom": "^4.4.0-beta.7",
    "react-select": "^2.4.1",
    "react-toggle": "^4.0.2",
    "react-tooltip": "^3.10.0",
    "recast-wasm": "^0.1.1",
    "signals": "^1.0.0",
    "three": "github:mozillareality/three.js#hubs/master",
    "three-mesh-bvh": "^0.1.2",
    "url-toolkit": "^2.1.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "ava": "^0.25.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "dotenv": "^6.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.12.4",
    "esm": "^3.2.10",
    "fibers": "^4.0.1",
    "file-loader": "^1.1.11",
    "get-chrome": "0.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jsdom": "^15.1.1",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "puppeteer-core": "^1.13.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "selfsigned": "^1.10.4",
    "standardized-audio-context-mock": "^7.0.1",
    "style-loader": "^0.21.0",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "worker-loader": "^2.0.0"
  }
}
